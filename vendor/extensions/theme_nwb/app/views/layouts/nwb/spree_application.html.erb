<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<%= render :partial => 'shared/head'%>
<%= render :partial => 'shared/gwo/control', :locals => {:eid => NWB_GLOBAL_TEST_EID, :domain_name => NWB_DOMAIN_NAME} %>
<%#= render :partial => 'shared/ga/tracking' %>
</head>

<script>utmx_section('body_tag')</script>
<body>
<%= render :text=>"</noscript>" %>

  <div id="container">
    <%= render "/shared/nwb_navigation" %>
    <div id="main">
      <%= render 'shared/nwb_sidebar' %>
      <div id="body">
        <div class="flash notice" style="display:none;"></div>
        <div class="flash errors" style="display:none;"></div>

        <%= yield :breadcrumbs %>
        <%= yield %>
      </div>
    </div>
    <hr />
    <%= render :partial => "shared/nwb_footer" %>
  </div>
  <%= render 'shared/google_analytics' %>
  <%= render 'shared/ppc' %>
  <%= yield :analytics %>
</body>

<% (1..NWB_GLOBAL_TEST_JS_COUNT.to_i).each do |i| %>
  <script>utmx_section('global_test_js_<%= i.to_s %>')</script>
  <%= render :text=>"</noscript>" %>
<% end %>

<!-- block being tested -->
<script type="text/javascript">
  if(document.body.className=='guarantee_var_1')
  {
    var text = $(".content:first").html();
    text = text.replace('90-Day', '365-Day', 'gm');
    text = text.replace('90 Day', '365 Day', 'gm');
    text = text.replace('90-day', '365-day', 'gm');
    text = text.replace('90 day', '365 day', 'gm');
    $(".content:first").html(text);
  }
</script>
<!-- end of block being tested -->
    
<%= render :partial => 'shared/gwo/tracking', :locals => {:eid => NWB_GLOBAL_TEST_EID, :pid => NWB_GLOBAL_TEST_PID, :domain_name => NWB_DOMAIN_NAME} %>
</html>

